  "scripts": {
    "astro": "astro",
    "build": "astro build",
    "cf-typegen": "wrangler types",
    "check": "npm run build && wrangler deploy --dry-run",
    "db:migrate": "wrangler d1 migrations apply DB --local",
    "db:migrate:remote": "wrangler d1 migrations apply DB --remote",
    "deploy": "npm run build && npx wrangler deploy",
    "dev": "npm run db:migrate && npm run build && npx wrangler dev --port 4321",
    "predeploy": "npm run db:migrate:remote",
    "preview": "npm run build && wrangler dev",
    "migrate": "node scripts/migrate.js --apply",
    "migrate:dry-run": "node scripts/migrate.js --dry-run",
    "migrate:test": "node scripts/test-migrations.js",
    "db:setup": "npm run migrate && npm run migrate:test"
  }
